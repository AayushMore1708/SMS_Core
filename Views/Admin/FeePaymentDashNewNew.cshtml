@model SMS_Core.Models.PrimEngFee
@{
    // Layout = null;
   @* Html.EnableClientValidation(); *@
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="keywords" content="jQuery Splitter, Splitter Widget, Splitter, jqxSplitter" />
    <meta name="description" content="This page demonstrates splitter's events" />
    <title id='Description'>Fee Details</title>
    <style>
        html, body {
            height: 100%;
        }

        .MyCssClassD
{
  background:url(@Url.Content("~/Content/ico/delete.png")) no-repeat;display:block; height:24px; width:24px;
}

        .MyCssClassE
{
  background:url(@Url.Content("~/Content/ico/edit.png")) no-repeat;display:block; height:24px; width:24px;
}


    </style>



    <link href="~/Content/css/dataTables.bootstrap.css" rel="stylesheet" />
</head>
<body class='default'>


    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th><b>Fee Receivable</b></th>
                        <th><b>Fee Received Fee Counter</b></th>
                        <th><b>Fee Received Manual Receipt</b></th>
                        <th><b>Discount</b></th>
                        <th><b>Excess Paid</b></th>
                        <th><b>Fee Waived / RTE</b></th>
                        <th><b>Balance</b></th>
                    </tr>
                </thead>
                <tbody>
                   @if (Model != null)
                   {
                    <tr>
                        <td>@Model.FEESRECEIVABLEFOR222023</td>
                        @* <td>@Model.FEESRECEIVEDONFEECOUNTER222023</td> *@
                        <td>@Model.FEESRECEIVEDONMANUALRECEIPTS222023</td>
                        @* <td>@Model.DISCOUNTSASPERSUPREMECOURTORDER222023</td> *@
                        @* <td>@Model.EXCESSPAID222023</td> *@
                        @* <td>@Model.FEESWAIVEDOFFRTESTAFFWARDS222023</td> *@
                        <td>
                            @if (@Model.BALANCE222023 != "")
                            {
                                <a href="javascript:checkedinDl(@Model.FeeIDNo)">@Model.BALANCE222023</a>
                                 }
                                else
                                {
                                @Model.BALANCE222023
                                }
                            </td>
                    </tr>
                   }
                </tbody>
            </table>

            
        </div>
    </div>


</body>
</html>
<script  >
    function checkedinDl(id) {
        
        var url = "http://154.61.72.250:8083/Admin/balanceDetail/" + id
        window.open(url, "_blank", "toolbar=no,scrollbars=yes,resizable=yes,top=200,left=400,width=1200,height=600");

    }
   
</script>