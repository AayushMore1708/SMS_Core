

@{
    ViewBag.Title = "View Fee Receipt";
   @* Html.EnableClientValidation(); *@
    //Layout = null;
}
<script type="text/javascript">
    $(document).ready(function () {
        $('#footer').hide();

        $("#startChallanDate").datepicker({

            changeMonth: true,
            changeYear: true,
            dateFormat: 'dd-M-yy'
        });
        $("#startChallanDate").datepicker("setDate", new Date());

        $("#endChallanDate").datepicker({
            changeMonth: true,
            changeYear: true,
            dateFormat: 'dd-M-yy'
        });
        $("#endChallanDate").datepicker("setDate", new Date());
    });

</script>
 


    
    <div class="panel">
        <div class="panel-body">

            <div class="form-group">
                <label class="control-label col-lg-2">From Date</label>
                <div class="col-lg-4">
                    @Html.TextBox("startChallanDate", null, new { @Class = "form-control input-sm" })
                </div>
            </div>
            <div class="form-group">
                <label class="control-label col-lg-2">To Date</label>
                <div class="col-lg-4">
                    @Html.TextBox("endChallanDate", null, new { @Class = "form-control input-sm" })
                </div>

            </div>

            <div class="form-group">
                <label class="control-label col-lg-2">Class</label>
                <div class="col-lg-4">
                    @Html.DropDownList("FeeCourseName", new SelectList(ViewBag.crc, "AcadamicCourceName", "AcadamicCourceName"), "", new { @class = "form-control input-sm" })
                </div>

            </div>

            <div class="form-group">
                <label class="control-label col-lg-2">Batch Name</label>
                <div class="col-lg-4">
                    @Html.DropDownList("division", new SelectList(ViewBag.bat, "BatchName", "BatchName"), "", new { @class = "form-control input-sm" })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-lg-2">Section </label>
                <div class="col-lg-4">
                    @Html.DropDownList("grtype", new SelectList(ViewBag.sect, "GRType", "GRType"), "", new { @class = "form-control input-sm" })
                </div>
            </div>

            <div class="form-group">
                <label class="control-label col-lg-2">Class Teacher</label>
                <div class="col-lg-4">
                    @Html.DropDownList("ClassTeacher", new SelectList(ViewBag.classT, "Ename", "Ename"), "", new { @class = "form-control input-sm" })
                </div>
            </div>


            <div class="form-group">
                <label for="tags" class="control-label col-lg-2">&nbsp;</label>
                <div class="col-lg-4">
                    <input type="submit" id="frmsave" value="View Fee Report" class="btn btn-primary" />
                </div>
            </div>
        </div>
    </div>


<div id="challanview">
 
    <div class="col-lg-12">

    </div>

</div>
 
<br />
 
 


